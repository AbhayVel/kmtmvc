
<html>
<head>
    <title></title>
    <script></script>
</head>
<body>
    <p><h2>Index</h2></p>
    <table>
        @for (int i = 0; i <= 5; i++)
        {
            <tr>
                <td>@i</td>
            </tr>
            <tr>
                <td>@(i+1)</td>
            </tr>
        }
    </table>
</body>
</html>


